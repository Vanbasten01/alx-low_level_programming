#include <stdio.h>
/**
 *main - Entry point
 *Description: "print all possible different combinations of three digits"
 *Return: Always 0
 */

int main(void)
{
	int a = '0';
	int b = '1';
	int c = '2';

	while (a <= '9',b <= '8', c <= '9')
	{
		if (!(a > b && a > c && b > c))
		{
			putchar(a);
			putchar(b);
			putchar(c);
			a++;
			b++;
			c++;
			if (a == '7' && b == '8' && c ==9)
			{
				putchar('\n');
			}
			else
			{
				putchar(',');
				putchar(' ');
			
			}
		}
	}
	return (0)
}
